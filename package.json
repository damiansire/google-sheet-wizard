{
  "name": "google-sheet-wizard",
  "version": "0.0.0",
  "description": "Library for easy interaction with Google Sheets. Read, write, and update data effortlessly. Simplify your workflows and automate tasks.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "clean": "rmdir /s /q dist",
    "test": "jest --testMatch \"**/src/**/*.spec.ts\"",
    "build": "npm run clean && tsc",
    "build-test": "npm run build && jest --testMatch \"**/dist-test/builded-package.spec.ts\"",
    "prepublishOnly": "npm run build-test"
  },
  "keywords": [
    "google sheets",
    "node.js",
    "npm package",
    "data manipulation",
    "spreadsheet",
    "automation",
    "javascript",
    "typescript"
  ],
  "author": "Damian Sire <damiansirecontacto@gmail.com> (https://damiansire.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/damiansire/google-sheet-wizard.git"
  },
  "bugs": {
    "url": "https://github.com/damiansire/google-sheet-wizard/issues"
  },
  "homepage": "https://github.com/damiansire/google-sheet-wizard#readme",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}